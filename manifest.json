{
	"name": "are.na channel search",
	"description" : "To use, type 'arena' plus your channel name into the search bar.",
	"version": "1.0.2",
	"background": {
	  "scripts": ["background.js"]
	},
	"omnibox": { "keyword" : "arena" },
	"manifest_version": 2,
	"permissions": [
	  "activeTab",
	  "storage"
	],	
	"options_ui": {
		"page": "options/options.html"
	  },	
	"icons": {
		"48": "icons/icon-arena-search.svg"
	},
	"browser_specific_settings": {
		"gecko": {
		  "id": "{0a578fda-88e8-464c-9de2-40d684adc225}",
		  "strict_min_version": "42.0"
		}
	}
  }