{
	"name": "are.na channel search",
	"description" : "To use, type 'arena' plus your channel name into the search bar.",
	"version": "1.0.1",
	"background": {
	  "scripts": ["background.js"]
	},
	"omnibox": { "keyword" : "arena" },
